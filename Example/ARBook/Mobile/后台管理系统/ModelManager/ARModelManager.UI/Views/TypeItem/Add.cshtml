@section header
{
    <link rel="stylesheet" href="~/Content/admin/css/treetable.css" />
    <style>
        .layui-btn + .layui-btn {
            margin-left: inherit !important;
        }
        #sizesmodel input {
            text-align:center;
        
        }
        i.selected {
            color:red;
        }
        textarea {
            text-align:left;        
        }
